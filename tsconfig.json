{
    "compilerOptions": {
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "sourceMap": true,
        "jsx": "react",
        "target": "es5",
        "downlevelIteration": true,
        "lib": ["dom", "es2018"],
        "allowSyntheticDefaultImports": true
    },
    "include": ["**/*"],
    "exclude": ["node_modules"]
}
